    "express": "^4.17.1",
    "express-device": "^0.4.2",
    "express-fileupload": "^1.1.7-alpha.4",
    "express-session": "^1.17.1",
    "fs": "0.0.1-security",
    "http": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.2.1",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "redis": "^3.1.1",
    "speakeasy": "^2.0.0",
    "useragent": "^2.3.0",
    "winston": "^3.3.3"
